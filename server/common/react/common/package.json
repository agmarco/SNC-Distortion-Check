{
  "name": "cirs-common",
  "version": "0.0.6",
  "description": "CIRS Common",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "typings": "./index.d.ts",
  "files": [
    "es",
    "lib",
    "src",
    "index.d.ts"
  ],
  "author": "Innolitics",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/react": "^15.0.14",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.23.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.2.4",
    "date-fns": "^1.28.2",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "react": "^15.4.2",
    "typescript": "^2.2.1"
  },
  "npmName": "cirs-common",
  "engines": {
    "node": "7.7.4"
  },
  "scripts": {
    "start": "yarn run clean && yarn run build",
    "clean": "rm -rf build es lib",
    "build": "yarn run build:tsc && yarn run build:es && yarn run build:commonjs",
    "build:tsc": "tsc --rootDir src --outDir build",
    "build:es": "babel build --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel build --out-dir lib"
  }
}
